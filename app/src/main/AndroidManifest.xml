<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmail.maryayolansia.worldlistconten">

    <permission android:name="com.gmail.maryayolansia.worldlistconten.PERMISSION" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.gmail.maryayolansia.worldlistconten.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name="com.gmail.maryayolansia.worldlistconten.EditWordActivity">
        </activity>
        <provider
            android:name=".WordListContentProvider"
            android:authorities="com.gmail.maryayolansia.worldlistconten.provider"
            android:exported="true">
        </provider>
    </application>

</manifest>